#include "dubhe.dtsi"

/ {
	model = "StarFive Dubhe Dual-FPGA";

	aliases {
		serial0 = &uart0;
	};

	chosen {
		bootargs = "console=ttySIF0,115200 earlycon=sbi";
	};

	cpus {
		timebase-frequency = <5000000>;
	};

	memory@80000000 {
		device_type = "memory";
		reg = <0x0 0x80000000 0x2 0x0>;
	};

	soc {
	};
};

&spi0 {
	status = "okay";
	mmc@0 {
		compatible = "mmc-spi-slot";
		reg = <0>;
		spi-max-frequency = <20000000>;
		voltage-ranges = <3300 3300>;
		disable-wp;
	};
};

&qspi1 {
	status = "okay";
};

&uart0 {
	status = "okay";
};

&pbus_clk {
	clock-frequency = <10000000>;
};
